{
    "name": "mayoz/blockchain",
    "description": "A simple example to understand the Blockchain algorithm.",
    "keywords": ["cryptocurrency", "blockchain", "algorithm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sercan Cakir",
            "email": "srcnckr@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mayoz\\Blockchain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mayoz\\Blockchain\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
